{
  "name": "league",
  "version": "1.0.0",
  "description": "motogp league",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=dev && npm-run-all --parallel dev-server dev-css",
    "dev-server": "lite-server index.html --serveStatic json",
    "dev-css": "node-sass-chokidar src/ -o src/ --watch --recursive --source-map true",
    "test": "jest --watch",
    "build:server": "lite-server --config bs-config.prod.js",
    "build:parcel": "parcel build index.prod.html --public-url ./",
    "build:css": "node-sass-chokidar src/ -o src/ --recursive --source-map true",
    "build": "npm-run-all -p build:*",
    "debug-prod": "npm-run-all --parallel dev-css prod",
    "prod": "parcel index.prod.html"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.motogp.net:axelclaver/league.git"
  },
  "keywords": [
    "state",
    "management",
    "es6",
    "testing"
  ],
  "author": "Dorna Sports",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^22.1.1",
    "lite-server": "^2.3.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.2.1",
    "postcss-modules": "^1.1.0",
    "regenerator-runtime": "^0.11.1"
  },
  "dependencies": {
    "promise-polyfill": "^7.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
